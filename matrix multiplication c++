#include<iostream>
using namespace std;
int main()
{int i,j,k,r1,c1,r2,c2,a[10][10],b[10][10],result[10][10];
cout<<"enter the no of rows and cols of first array a :";
cin>>r1>>c1;
cout<<"enter the elements of first array a :";
for(i=0;i<r1;i++)
for(j=0;j<c1;j++)
cin>>a[i][j];

cout<<"enter the no of rows and cols of second array b :";
cin>>r2>>c2;
cout<<"enter the elements of second array b :";
for(i=0;i<r2;i++)
for(j=0;j<c2;j++)
cin>>b[i][j];

if(c1!=r2)
{cout<<"multiplication is not possible";
return 0;}

for(i=0;i<r1;i++)
for(j=0;j<c2;j++)
result[i][j]=0;
for(k=0;k<c1;k++)
result[i][j]=a[i][k]*b[k][j];

cout<<"the resultant matrix after multiplication is:";
{for(i=0;i<r1;i++)
{for(j=0;j<c2;j++)
cout<<result[i][j]<<" ";}
cout<<endl;}
}

